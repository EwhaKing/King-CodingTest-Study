#include <string>
#include <vector>

using namespace std;
int answer = 0;
void findexecution(vector<int> numbers, int target,int sum, int count){
    
    if(numbers.size()==count){//숫자가 모두 사용되고
        if(sum==target) answer++;//그 숫자들의 합이 target의 값과 같으면 가능한 경우의 하나 더하기
        return;
        
    }
    
    findexecution(numbers,target,sum+numbers[count],count+1);//수를 더하는 경우
    findexecution(numbers,target,sum-numbers[count],count+1);//수를 빼는 경우
    
    
}


int solution(vector<int> numbers, int target) {
    findexecution(numbers,target,0,0);
    return answer;
}
